{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug star_walking",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/ex05/star_walking",
            "args": ["4"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/ex05",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "build",
            "miDebuggerPath": "/usr/bin/gdb",
            "logging": {
                "engineLogging": false,
                "trace": true,
                "traceResponse": true
            },
            "osx": {
                "MIMode": "lldb"
            },
            "windows": {
                "MIMode": "gdb",
                "miDebuggerPath": "C:/mingw/bin/gdb.exe"
            }
        }
    ]
}
